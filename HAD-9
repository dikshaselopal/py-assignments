#HAD-9 Q6
import random
score=0
for i in range(5):
  a=int(input("enter the number :"))
  computer=random.randint(0,11)
  if(a==computer):
    print("great your guess is correct")
    score+=10
  else:
    print("sorry your number is incorrect")
    score-=1
print("your score "+str(score))





