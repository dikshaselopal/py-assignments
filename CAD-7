#CAD-7 Q2:
input1=input("enter the first string: ")
input2=input("enter the second string: ")
strings=(input1+input2)
print(strings)
userinput=int(input("enter the no. you want to repeat the string: "))
print(strings*userinput)

#CAD-7 Q3:
input1=input("enter the first string: ")
input2=input("enter the second string: ")
strings=[]
strings.append(input1)
strings.append(input2)
print(strings)
userinput=int(input("enter the no. you want to repeat the string: "))
print(strings*userinput)

